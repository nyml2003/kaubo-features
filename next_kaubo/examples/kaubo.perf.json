{
    "version": "1.0",
    "profile": "trace",
    "runtimeOptions": {
        "limits": {
            "maxStackSize": 102400,
            "maxRecursionDepth": 1024
        },
        "vm": {
            "initialStackSize": 1024,
            "initialFramesCapacity": 256,
            "inlineCacheCapacity": 256
        },
        "coroutine": {
            "initialStackSize": 1024,
            "initialFramesCapacity": 256
        }
    }
}
